{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python Debugger: Django",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
            "runserver",
            "--noreload",
            "127.0.0.1:8005",
        ],
        "django": true,
        "justMyCode": true
      }
    ]
  }